hidden = [".config", "node_modules", ".env"]
run = "bash replit-init.sh"
entrypoint = "server/index.ts"

[nix]
channel = "stable-23_11"

[deployment]
run = ["bash", "replit-init.sh"]
build = ["npm", "run", "build"]

[packager]
language = "typescript"

[packager.features]
packageSearch = true
guessImports = true
enabledForHosting = true